generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tb_user {
  email    String    @id @db.VarChar(255)
  nickname String?   @db.VarChar(255)
  password String?   @db.VarChar(255)
  sessions Session[]
}

model Session {
  id        String   @id
  userId    String
  token     String
  expiresAt DateTime

  user tb_user @relation(references: [email], fields: [userId], onDelete: Cascade)
}
